{
  "name": "crs-binding",
  "description" : "debug tool for crs-binding",
  "version": "0.0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/inject.js"],
      "run_at": "document_idle"
    }
  ],
  "web_accessible_resources": [
    "js/content.js"
  ]
}